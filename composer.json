{
  "name": "hareland/multi-cache-remember",
  "description": "This package offers the rememberMany macro for the Laravel Cache facade, providing efficient retrieval of multiple cache keys with customizable expiration and callbacks for missing keys.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "hareland",
      "email": "packagist@proton.me",
      "homepage": "https://github.com/hareland"
    }
  ],
  "autoload": {
    "psr-4": {
      "Hareland\\MultiCacheRemember\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hareland\\MultiCacheRemember\\Tests\\": "tests/"
    }
  },
  "require": {
    "illuminate/support": "^6|^7|^8|^9|^10"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^9.0",
    "pestphp/pest": "^1.22",
    "pestphp/pest-plugin-laravel": "^1.3",
    "orchestra/testbench": "^7.10"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hareland\\MultiCacheRemember\\MultiCacheServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
